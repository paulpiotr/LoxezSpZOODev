@model LoxezSpZOOContext.Models.KoszykElement
@{
    ViewData["Title"] = "Usuń z koszyka";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Usuń z koszyka</h1>
<h3>Usunąć @Html.DisplayFor(model => model.Nazwa)?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.KoszykNaglowekId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.KoszykNaglowekId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nazwa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nazwa)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TwTowarTwId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TwTowarTwId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataUtworzenia)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataUtworzenia)
        </dd>
    </dl>
    <form asp-action="UsunZKoszyka">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Usuń z koszyka" class="btn btn-danger" />
        <a asp-controller="KoszykElement" asp-action="PobierzKoszyk" class="btn btn-info">Powrót do koszyka</a>
    </form>
</div>